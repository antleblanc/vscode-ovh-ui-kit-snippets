    "prefix": "oui-select-picker",
    "body": [
      "<oui-select-picker",
      "\tdisabled=\"\\$ctrl.${1:disabled}\"",
      "\tname=\"${1:ouiSelectPicker1}\"",
      "\tdata-match=\"${2:name}\"",
      "\tdata-model=\"\\$ctrl.${3:model}\"",
      "\tdata-label=\"${4:Value A or B}\"",
      "\tdata-on-change=\"\\$ctrl.${7:onChange(modelValue)}\"",
      "\tdata-values=\"\\$ctrl.${8:values1}\">",
      "</oui-select-picker>",
      "<oui-select-picker",
      "\tdisabled=\"\\$ctrl.disabled\"",
      "\tname=\"${1:ouiSelectPicker1}\"",
      "\tdata-match=\"${2:name}\"",
      "\tdata-model=\"$ctrl.${3:model}\"",
      "\tdata-label=\"${5:Value C}\"",
