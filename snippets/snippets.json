      "\tdisabled=\"\\$ctrl.${1:disabled}\"",
      "\tdata-model=\"\\$ctrl.${2:model}\"",
      "\tname=\"${3:oui-radio-1}\"",
      "\tdata-on-change=\"\\$ctrl.${4:onChange(modelValue)}\"",
      "\tvalue=\"\\$ctrl.${5:value1}\">",
